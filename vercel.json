{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
{
  "headers": [
    {
      "source": "/display/(.+)/(.+)/(\d+)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM *"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *"
        }
      ]
    }
  ]
}
}
